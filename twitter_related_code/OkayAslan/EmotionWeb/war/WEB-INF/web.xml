<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Twoogle.html</welcome-file>
  </welcome-file-list>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>EmotionWebServlet</servlet-name>
    <servlet-class>emotion.server.EmotionWebServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>EmotionWebServlet</servlet-name>
    <url-pattern>/emotionweb/emotion</url-pattern>
  </servlet-mapping>
  

</web-app>
